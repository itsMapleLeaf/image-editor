    "eslint-plugin-mobx": "^0.0.8",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.3",
    "ts-unused-exports": "^8.0.0",
