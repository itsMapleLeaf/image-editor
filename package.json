    "@mdi/js": "^6.7.96",
    "@mdi/react": "^1.6.0",
    "@popperjs/core": "^2.11.5",
    "clsx": "^1.1.1",
    "mobx": "^6.6.0",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-use-measure": "^2.1.1"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.4",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/eslint": "^8.4.3",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
